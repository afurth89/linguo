extends ../layout
block nav
	include ../includes/_nav
block content
	h1 Users Index Page
	each user in users
		div(class="list-group")
			h4(class = "list-group-item-heading") #{user.name}
			p(class = "list-group-item-text"): li Description: #{user.description}
			li: a(href="/threads") Message this User!